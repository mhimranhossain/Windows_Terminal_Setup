{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "explorer.sortOrder": "type",
  "workbench.layoutControl.type": "menu",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "terminal.integrated.cursorBlinking": true,
  "editor.minimap.renderCharacters": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "files.enableTrash": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "left",
  "editor.minimap.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "telemetry.telemetryLevel": "off",
  "extensions.ignoreRecommendations": true,
  "scm.diffDecorationsGutterVisibility": "hover",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.showPushSuccessNotification": true,
  "editor.fontFamily": " CaskaydiaCove NF mono, Cascadia code , Consolas",
  "terminal.integrated.fontFamily": "CaskaydiaCove NF mono, Cascadia Mono, Consolas",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontLigatures": true,
  "files.associations": {
    "*.json": "jsonc",
    "*.js": "javascript",
    "*.css": "tailwindcss",
    "*.shl": "cpp"
  },
  "editor.codeLens": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "other": "on",
    "strings": "on",
    "comments": "on"
  },
  "git.confirmSync": false,
  "explorer.incrementalNaming": "smart",
  "debug.inlineValues": "on",
  "editor.suggest.showInlineDetails": false,
  "window.autoDetectColorScheme": true,
  "errorLens.statusBarColorsEnabled": true,
  "workbench.editor.splitInGroupLayout": "vertical",
  "debug.internalConsoleOptions": "neverOpen",
  "debug.openDebug": "neverOpen",
  "debug.terminal.clearBeforeReusing": true,
  "css.lint.duplicateProperties": "warning",
  "less.lint.duplicateProperties": "warning",
  "scss.lint.duplicateProperties": "warning",
  "editor.suggest.showDeprecated": false,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "problems.showCurrentInStatus": true,
  "emmet.triggerExpansionOnTab": true,
  "editor.detectIndentation": false,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconSize": "80%",
  "workbench.reduceMotion": "off",
  "workbench.editor.highlightModifiedTabs": true,
  "scm.alwaysShowRepositories": true,
  "scm.alwaysShowActions": true,
  "git.decorations.enabled": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "errorLens.fontStyleItalic": true,
  "editor.foldingMaximumRegions": 50000,
  "javascript.format.semicolons": "remove",
  "javascript.suggest.completeJSDocs": false,
  "terminal.integrated.allowChords": false,
  "terminal.integrated.altClickMovesCursor": false,
  "terminal.integrated.cursorWidth": 3,
  "typescript.format.semicolons": "remove",
  "css.lint.unknownAtRules": "ignore",
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.includeLanguages": {
    "html": "html",
    "javascript": "javascript",
    "css": "css"
  },
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "git.postCommitCommand": "push",
  "editor.inlineSuggest.enabled": false,
  "audioCues.lineHasInlineSuggestion": "off",
  "terminal.integrated.confirmOnKill": "never",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "editor.guides.bracketPairs": true,
  "git.allowForcePush": true,
  "explorer.openEditors.visible": 0,
  "search.defaultViewMode": "tree",
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "debug.console.fontFamily": " CaskaydiaCove NF mono, Cascadia code , Consolas",
  "files.autoSave": "afterDelay",
  "explorer.compactFolders": false,
  "scm.defaultViewMode": "tree",
  "scm.countBadge": "off",
  "files.exclude": {
    "**/.git": false,
    "**/.ignore": true,
    "**/desktop.ini": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "args": [
        "-NoLogo"
      ]
    }
  },
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.minimumContrastRatio": 1,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "emmet.excludeLanguages": [
    "markdown",
    "php"
  ],
  "browse-lite.startUrl": "http://localhost:3000/ ",
  "browse-lite.verbose": true,
  "diffEditor.wordWrap": "off",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.mouseWheelScrollSensitivity": 3,
  "terminal.integrated.tabs.hideCondition": "never",
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "breadcrumbs.enabled": false,
  "errorLens.gutterIconsEnabled": true,
  "thunder-client.saveToWorkspace": true,
  "errorLens.enabled": true,
  // "workbench.colorCustomizations": {
  //   "statusBar.background": "#1A1A1A",
  //   "statusBar.noFolderBackground": "#212121",
  //   "statusBar.debuggingBackground": "#263238",
  // },
  "editor.mouseWheelZoom": true,
  "html.format.wrapLineLength": 0,
  "html.format.wrapAttributes": "preserve",
  "html.format.templating": true,
  "svelte.plugin.svelte.format.config.singleQuote": true,
  "svelte.plugin.svelte.format.config.svelteBracketNewLine": false,
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.note-new-transformation": false,
  "window.menuBarVisibility": "compact",
  "color-highlight.matchRgbWithNoFunction": true,
  "workbench.preferredDarkColorTheme": "After Dark",
  "workbench.preferredLightColorTheme": "After Dark",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "markdown.occurrencesHighlight.enabled": true,
  "markdown.preview.breaks": true,
  "markdown.validate.enabled": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "json.trace.server": "messages",
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "editor.inlayHints.padding": true,
  "debug.console.acceptSuggestionOnEnter": "on",
  "debug.showInStatusBar": "never",
  "debug.showSubSessionsInToolBar": true,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.tabs.enabled": false,
  "terminal.explorerKind": "external",
  "terminal.external.windowsExec": "wt",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "workbench.colorTheme": "After Dark",
  "editor.tokenColorCustomizations": {
    "comments": "#6c8093"
  },
  "editor.foldingHighlight": false,
  "reactSnippets.settings.importReactOnTop": false,
  "update.showReleaseNotes": false,
  "color-highlight.matchWords": true,
  "window.zoomLevel": 3,
  "files.eol": "\n",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoImportUserSymbols": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.typeCheckingMode": "basic",
  "vsintellicode.features.python.deepLearning": "enabled",
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "npm",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "eslint.options": {
    "overrideConfig": {
      "env": {
        "commonjs": true,
        "browser": true,
        "node": true,
        "es6": true,
        "worker": true
      },
      "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "impliedStrict": true,
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "rules": {
        "no-debugger": "off",
        "no-extra-semi": "warn",
        "no-const-assign": "warn",
        "no-compare-neg-zero": "warn",
        "no-constant-condition": "warn",
        "no-duplicate-case": "warn",
        "no-duplicate-imports": "warn",
        "no-ex-assign": "warn",
        "no-fallthrough": "warn",
        "no-func-assign": "warn",
        "no-import-assign": "warn",
        "no-inner-declarations": "warn",
        "no-loss-of-precision": "warn",
        "no-self-assign": "warn",
        "no-self-compare": "warn",
        "no-setter-return": "warn",
        "no-sparse-arrays": "warn",
        "no-undef": "warn",
        "no-unexpected-multiline": "warn",
        "no-unmodified-loop-condition": "warn",
        "no-unreachable": "warn",
        "no-unsafe-optional-chaining": "warn",
        "no-unsafe-negation": "warn",
        "no-use-before-define": "warn",
        "use-isnan": "warn",
        "valid-typeof": "warn",
        "accessor-pairs": "warn",
        "arrow-body-style": "warn",
        "curly": "warn",
        "no-confusing-arrow": "warn",
        "class-methods-use-this": "warn",
        "block-scoped-var": "warn",
        "eqeqeq": "warn",
        "func-names": "warn",
        "no-floating-decimal": "warn",
        "no-global-assign": "warn",
        "no-label-var": "warn",
        "no-lonely-if": "warn",
        "no-loop-func": "warn",
        "no-mixed-operators": "warn",
        "no-param-reassign": "warn",
        "no-redeclare": "warn",
        "no-return-assign": "warn",
        "no-return-await": "warn",
        "no-shadow": "warn",
        "no-undef-init": "warn",
        "no-unused-labels": "warn",
        "no-unneeded-ternary": "warn",
        "no-useless-call": "warn",
        "no-useless-catch": "warn",
        "prefer-const": "warn",
        "no-useless-return": "warn",
        "prefer-spread": "warn",
        "vars-on-top": "warn",
        "yoda": "warn",
        "strict": "error"
      }
    }
  }
}