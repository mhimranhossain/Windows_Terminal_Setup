{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "explorer.sortOrder": "type",
  "workbench.layoutControl.type": "menu",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "terminal.integrated.cursorBlinking": true,
  "editor.minimap.renderCharacters": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "files.enableTrash": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "left",
  "editor.minimap.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "telemetry.telemetryLevel": "off",
  "extensions.ignoreRecommendations": true,
  "scm.diffDecorationsGutterVisibility": "hover",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.showPushSuccessNotification": true,
  "editor.fontFamily": " CaskaydiaCove NF mono, Cascadia code , Consolas",
  "terminal.integrated.fontFamily": "CaskaydiaCove NF mono, Cascadia Mono, Consolas",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontLigatures": true,
  "files.associations": {
    "*.json": "jsonc",
    "*.js": "javascript",
    "*.css": "css",
    "*.shl": "cpp"
  },
  "editor.codeLens": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "other": "on",
    "strings": "on",
    "comments": "on"
  },
  "git.confirmSync": false,
  "explorer.incrementalNaming": "smart",
  "debug.inlineValues": "on",
  "editor.suggest.showInlineDetails": false,
  "window.autoDetectColorScheme": true,
  "errorLens.statusBarColorsEnabled": true,
  "workbench.editor.splitInGroupLayout": "vertical",
  "debug.internalConsoleOptions": "neverOpen",
  "debug.openDebug": "neverOpen",
  "debug.terminal.clearBeforeReusing": true,
  "css.lint.duplicateProperties": "warning",
  "less.lint.duplicateProperties": "warning",
  "scss.lint.duplicateProperties": "warning",
  "editor.suggest.showDeprecated": false,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "problems.showCurrentInStatus": true,
  "emmet.triggerExpansionOnTab": true,
  "editor.detectIndentation": false,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconSize": "80%",
  "workbench.reduceMotion": "off",
  "workbench.editor.highlightModifiedTabs": true,
  "scm.alwaysShowRepositories": true,
  "scm.alwaysShowActions": true,
  "git.decorations.enabled": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "errorLens.fontStyleItalic": true,
  "editor.foldingMaximumRegions": 50000,
  "javascript.format.semicolons": "remove",
  "javascript.suggest.completeJSDocs": false,
  "terminal.integrated.allowChords": false,
  "terminal.integrated.altClickMovesCursor": false,
  "terminal.integrated.cursorWidth": 3,
  "typescript.format.semicolons": "remove",
  "css.lint.unknownAtRules": "ignore",
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "toggle",
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.includeLanguages": {
    "html": "html",
    "javascript": "javascript",
    "css": "css"
  },
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "git.postCommitCommand": "push",
  "editor.inlineSuggest.enabled": false,
  "audioCues.lineHasInlineSuggestion": "off",
  "editor.inlayHints.enabled": "off",
  "terminal.integrated.confirmOnKill": "never",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "editor.guides.bracketPairs": true,
  "git.allowForcePush": true,
  "explorer.openEditors.visible": 0,
  "search.defaultViewMode": "tree",
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "debug.console.fontFamily": " CaskaydiaCove NF mono, Cascadia code , Consolas",
  "files.autoSave": "afterDelay",
  "explorer.compactFolders": false,
  "scm.defaultViewMode": "tree",
  "scm.countBadge": "off",
  "files.exclude": {
    "**/.git": false,
    "**/.ignore": true,
    "**/desktop.ini": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "args": [
        "-NoLogo"
      ]
    },
  },
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.minimumContrastRatio": 1,
  "simpleBrowser.focusLockIndicator.enabled": false,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "emmet.excludeLanguages": [
    "markdown",
    "php"
  ],
  "browse-lite.startUrl": "http://localhost:4000/ ",
  "browse-lite.verbose": true,
  "diffEditor.wordWrap": "off",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.mouseWheelScrollSensitivity": 3,
  "terminal.integrated.tabs.hideCondition": "never",
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "breadcrumbs.enabled": false,
  "errorLens.gutterIconsEnabled": true,
  "terminal.external.windowsExec": "wt.exe",
  "thunder-client.saveToWorkspace": true,
  "errorLens.enabled": true,
  // "workbench.colorCustomizations": {
  //   "statusBar.background": "#1A1A1A",
  //   "statusBar.noFolderBackground": "#212121",
  //   "statusBar.debuggingBackground": "#263238",
  // },
  "editor.mouseWheelZoom": true,
  "workbench.colorTheme": "After Dark",
  "html.format.wrapLineLength": 0,
  "html.format.wrapAttributes": "preserve",
  "html.format.templating": true,
  "prettier.semi": false,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "svelte.plugin.svelte.format.config.singleQuote": true,
  "svelte.plugin.svelte.format.config.svelteBracketNewLine": false,
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.note-new-transformation": false,
}